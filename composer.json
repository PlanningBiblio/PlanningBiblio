{
    "name": "planningbiblio/planningbiblio",
    "description": "Planning Biblio",
    "authors": [
        {
            "name": "Jerome Combes",
            "email": "jerome@planningbiblio.fr"
        },
        {
            "name": "Alex Arnaud",
            "email": "alex.arnaud@biblibre.com"
        },
        {
            "name": "Matthias Meusburger",
            "email": "matthias.meusburger@biblibre.com"            
        },
        {
            "name": "Sarah Deborde",
            "email": "sarah.deborde@biblibre.com"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4",
        "apereo/phpcas": "^1.3",
        "composer/composer": "^1.6",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.12",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "laminas/laminas-zendframework-bridge": "^1.6",
        "league/oauth2-client": "*",
        "mashape/unirest-php": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpmailer/phpmailer": ">=6.1.6",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "^3.4",
        "symfony/lock": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "*",
        "symfony/property-access": "3.4.*",
        "symfony/property-info": "3.4.*",
        "symfony/proxy-manager-bridge": "3.4.*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "3.4.*",
        "symfony/serializer": "3.4.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "johngrogg/ics-parser": "^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "3.4.*",
        "symfony/css-selector": "3.4.*",
        "symfony/debug-bundle": "3.4.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/stopwatch": "3.4.*",
        "symfony/web-profiler-bundle": "3.4.*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "ocramius/package-versions": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests",
            "PlanningBiblio\\": "src/PlanningBiblio",
            "Model\\": "src/Model"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
