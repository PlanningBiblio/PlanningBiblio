{
    "name": "planningbiblio/planningbiblio",
    "description": "Planning Biblio",
    "authors": [
        {
            "name": "Jerome Combes",
            "email": "jerome@planningbiblio.fr"
        },
        {
            "name": "Alex Arnaud",
            "email": "alex.arnaud@biblibre.com"
        },
        {
            "name": "Matthias Meusburger",
            "email": "matthias.meusburger@biblibre.com"            
        },
        {
            "name": "Sarah Deborde",
            "email": "sarah.deborde@biblibre.com"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4",
        "apereo/phpcas": "^1.3",
        "composer/composer": "^1.6",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "league/oauth2-client": "*",
        "mashape/unirest-php": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpmailer/phpmailer": ">=6.1.6",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/config": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "*",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/lock": "^4.4",
        "symfony/monolog-bundle": "*",
        "symfony/process": "^4.4",
        "symfony/property-access": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/proxy-manager-bridge": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/security-csrf": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-link": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests",
            "PlanningBiblio\\": "src/PlanningBiblio",
            "Model\\": "src/Model"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
