{
    "name": "planningbiblio/planningbiblio",
    "description": "Planning Biblio",
    "authors": [
        {
            "name": "Jerome Combes",
            "email": "jerome@planningbiblio.fr"
        },
        {
            "name": "Alex Arnaud",
            "email": "alex.arnaud@biblibre.com"
        },
        {
            "name": "Matthias Meusburger",
            "email": "matthias.meusburger@biblibre.com"            
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.0",
        "apereo/phpcas": "^1.3",
        "cron/cron-bundle": "^2.4",
        "doctrine/orm": "^2.5",
        "league/oauth2-client": "*",
        "mashape/unirest-php": "*",
        "phpmailer/phpmailer": ">=6.1.6",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "*",
        "symfony/config": "3.4.*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "^3.4",
        "symfony/lock": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "3.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "PlanningBiblio\\": "src/PlanningBiblio",
            "Model\\": "src/Model"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
